@using BakeryTracker.Models;

<h3>Here are all the orders for this vendor:</h3>

<ol>
@foreach (Order order in @Model["orders"])
{
  <li><a href='/categories/@Model["category"].Id/orders/@order.Id'>@order.Description</a></li>
}
</ol>

<p><a href='/vendors/@Model["vendor"].Id/vendors/new'>Add a New Order</a></p>
<p><a href='/vendors'>Return to Vendor List</a></p>
<p><a href='/'>Return to Homepage</a></p>